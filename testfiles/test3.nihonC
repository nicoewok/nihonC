#include <stdio.h>

整数 main() 「
    整数 i;

    //If statement
    整数 number = 10。
    もし (number は 5 と等しいかより大きい) 「
        printf("Number is greater or equal than 5\n")。
    」 それ以外 「
        printf("Number is 5 or less\n")。
    」

    // While loop
    整数 count は 0 です。
    while (count は 5 より小さい) 「
        printf("Count is %d\n", count)。
        count++。
    」

    // For loop
    整数 0 から 5 まで i 「
        printf("i is %d\n", i)。
    」
    

    return 0。
」